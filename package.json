{
  "name": "@observerly/useaestrium",
  "author": "Michael J. Roberts (observerly Ltd)",
  "version": "0.11.1",
  "description": "A composables Vue 3 library for using a remote observatory.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/observerly/useaestrium.git"
  },
  "prepublish": "tsc",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/observerly"
  },
  "main": "./dist/aestrium.es.js",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/aestrium.es.js",
  "exports": {
    ".": {
      "import": "./dist/aestrium.es.js",
      "require": "./dist/aestrium.umd.js"
    }
  },
  "unpkg": "./dist/aestrium.umd.js",
  "jsdelivr": "./dist/aestrium.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lib": "vue-tsc --noEmit && vite build --config vite.lib.config.ts",
    "preview": "vite preview",
    "test": "jest --config=jest.config.json",
    "prepublish": "npm run test && tsc",
    "lint": "eslint . --ext .ts"
  },
  "private": false,
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/cli-plugin-typescript": "^5.0.0-rc.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.2.4",
    "vite": "^2.7.8",
    "vue-tsc": "^0.30.1"
  },
  "dependencies": {
    "@observerly/celestia": "^0.19.1",
    "@vueuse/core": "^7.1.2",
    "vue": "^3.2.23"
  }
}
